all: pgsql-query-test

mysql-query-test_old: mysql-query-test_old.cpp common.o
	g++ -gstabs+ -o mysql-query-test_old mysql-query-test_old.cpp `pkg-config xvr2core xvr2db --cflags --libs` common.o

pgsql-query-test_old: pgsql-query-test_old.cpp common.o
	g++ -gstabs+ -o pgsql-query-test_old pgsql-query-test_old.cpp `pkg-config xvr2core xvr2db --cflags --libs` common.o

pgsql-query-test: pgsql-query-test_ng.cpp common_ng.o
	g++ -gstabs+ -o pgsql-query-test pgsql-query-test_ng.cpp `pkg-config xvr2core xvr2db --cflags --libs` common_ng.o

common.o: common.cpp common.h
	g++ -c -gstabs+ -o common.o common.cpp `pkg-config xvr2core xvr2db --cflags`

common_ng.o: common_ng.cpp common_ng.h
	g++ -c -gstabs+ -o common_ng.o common_ng.cpp `pkg-config xvr2core xvr2db --cflags`

clean:
	rm -f pgsql-query-test mysql-query-test *.o pgsql-query-test_ng

rebuild: clean all
